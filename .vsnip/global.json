{
  "log": {
    "prefix": ["log"],
    "body": "console.log(${1:$TM_SELECTED_TEXT});"
  },
  "useState()": {
    "prefix": "us",
    "body": "const [$1, set${1/(.*)/${1:/capitalize}/}] = useState($2);",
    "description": "useState()"
  },
  "inject": {
    "prefix": "inj",
    "body": "${1} = inject(${1/(.*)/${1:/pascalcase}/});",
    "description": "inject()"
  }
}
